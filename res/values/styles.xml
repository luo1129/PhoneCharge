<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="text_style_1" parent="android:Widget.Holo.TextView">
        <item name="android:textColor">#665d58</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="styleName" parent="android:Widget.Holo.Button">
        <item name="android:background">@drawable/login_button</item>
    </style>

    <style name="button_style_1" parent="android:Widget.Holo.Button">
        <item name="android:background">@drawable/button</item>
        <item name="android:textColor">@color/button_text_color</item>
    </style>

    <style name="edittext_style_1" parent="android:Widget.Holo.EditText">
        <item name="android:background">@drawable/edit_text_bg</item>
        <item name="android:ems">16</item>
        <item name="android:padding">4dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">#665d58</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="MyTheme" parent="android:Theme.Black.NoTitleBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

</resources>